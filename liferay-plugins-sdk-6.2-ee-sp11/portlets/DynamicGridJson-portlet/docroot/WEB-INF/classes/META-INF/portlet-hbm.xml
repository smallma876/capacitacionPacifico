<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.meera.model.Invheader" />
	<class name="com.meera.model.impl.InvheaderImpl" table="jqgrid_Invheader">
		<cache usage="read-write" />
		<id name="invId" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="ammount" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="tax" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="total" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="notes" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>